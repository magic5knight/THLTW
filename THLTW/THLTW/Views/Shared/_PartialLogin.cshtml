@using THLTW.Models
<div class="float-end" style="margin: 5px">
    @if (Session["TaiKhoan"] != null)
    {
        Customer cust = Session["TaiKhoan"] as Customer;
        <a href="#" class="btn btn-danger">@cust.NameCus</a>
        <a href="/Users/Logout" class="btn btn-primary">Logout</a>
    }
    else
    {
        <a href="/Users/Login" class="btn btn-primary">Sign in</a>
        <a href="/Users/Register" class="btn btn-success">Register</a>
    }
</div>
